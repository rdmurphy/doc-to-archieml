{
  "name": "@newswire/doc-to-archieml",
  "version": "1.0.0",
  "description": "Convert an ArchieML-formatted Google Doc directly into usable data using the Google Docs API.",
  "main": "index.js",
  "types": "types.d.ts",
  "files": [
    "index.js",
    "types.d.ts"
  ],
  "scripts": {
    "docs": "doctoc README.md --github",
    "git-pre-commit": "precise-commits",
    "prerelease": "yarn test",
    "release": "git push && git push --tags && yarn publish",
    "test": "mocha --timeout 5000 tests/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdmurphy/doc-to-archieml.git"
  },
  "keywords": [
    "googleapis",
    "archieml",
    "templating",
    "googledocs"
  ],
  "author": "Ryan Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdmurphy/doc-to-archieml/issues"
  },
  "homepage": "https://github.com/rdmurphy/doc-to-archieml#readme",
  "dependencies": {
    "archieml": "^0.4.2",
    "googleapis": "^39.2.0"
  },
  "devDependencies": {
    "@newswire/prettier-config": "^1.0.0",
    "@zeit/git-hooks": "^0.1.4",
    "doctoc": "^2.0.0",
    "mocha": "^8.1.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.17.1"
  },
  "prettier": "@newswire/prettier-config"
}
